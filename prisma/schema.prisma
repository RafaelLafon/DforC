// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
  output = "../node_modules/.prisma/client"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model User {
Tag String @unique
Name String
Email String
Status Int
Languages String
Serveurs String
Amis String
Password String
Parameters String


}

model Server {
  Id String @unique
  Name String
  Members String
  Admins String
  Channel Channel[]
}

model Channel {
  Id Int @id @default(autoincrement())
  Name String 
	Thread String
	Pinned String
	Moderation Boolean
  Server Server  @relation(fields: [ServerId], references: [Id])
  ServerId String
}